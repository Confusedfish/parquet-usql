CREATE ASSEMBLY ParquetDataLakeTools FROM "/ParquetDataLAkeTools.dll";
REFERENCE ASSEMBLY master.ParquetDataLakeTools;

@parquet = EXTRACT id   int,
                   bool_col bool
           FROM "/alltypes.plain.parquet"
           USING new ParquetDataLakeTools.ParquetExtractor();

OUTPUT @parquet
TO "/output/parquet.csv"
USING Outputters.Csv();

